# Development environment configuration
# Set to 'local' if you want to use localhost API
# Set to 'production' if you want to use the production API
VITE_API_ENV=local

# Local API configuration (only used when VITE_API_ENV=local)
VITE_API_HOST=localhost
VITE_API_PORT=8000

# Sentry configuration
# VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_SENTRY_ENVIRONMENT=development
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAY_SESSION_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAY_ERROR_SAMPLE_RATE=1.0 