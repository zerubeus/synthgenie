# Environment configuration example
# Copy this file to .env.development for local development
# Set these variables in Vercel for production deployment

# API configuration
VITE_API_ENV=production
VITE_API_HOST=your-api-host.com
VITE_API_PORT=443

# Sentry configuration
VITE_SENTRY_DSN=your-sentry-dsn-here
VITE_SENTRY_ENVIRONMENT=production
VITE_SENTRY_TRACES_SAMPLE_RATE=0.1
VITE_SENTRY_REPLAY_SESSION_SAMPLE_RATE=0.05
VITE_SENTRY_REPLAY_ERROR_SAMPLE_RATE=1.0